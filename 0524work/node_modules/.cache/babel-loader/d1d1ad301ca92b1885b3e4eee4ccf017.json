AAEA,QAAA,SAAA,EAAA;AAFA,OAPA;AAWA,MAAA,KAAA,EAAA;AACA,QAAA,IAAA,EAAA,CACA;AAAA,UAAA,SAAA,EAAA,YAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA,CADA;AAIA,QAAA,SAAA,EAAA,CACA;AAAA,UAAA,SAAA,EAAA,aAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SADA;AAJA;AAXA,KAAA;AAoBA,GA1CA;AA2CA,EAAA,OA3CA,qBA2CA;AACA,SAAA,QAAA;AACA,SAAA,MAAA;AACA,GA9CA;AA+CA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,WAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,MAAA,CAAA,CAAA;AACA,KAHA;AAIA;AACA,IAAA,MALA,oBAKA;AACA,WAAA,GAAA,GAAA,KAAA,IAAA,CAAA,MAAA,IAAA,GAAA,GAAA,GAAA,GAAA,GAAA;AACA,KAPA;AAQA,IAAA,UARA,sBAQA,QARA,EAQA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,MAAA,CAAA,IAAA,CAAA,QAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,SAAA;AAEA,cAAA,IAAA,GAAA,MAAA,CAAA,IAAA,CAAA,QAAA;AACA,cAAA,IAAA,GAAA,MAAA,CAAA,QAAA,CAAA,SAAA;AAEA,UAAA,GAAA,CAAA,sBAAA,EAAA;AACA;AACA,YAAA,WAAA,EAAA,IAFA;AAGA,YAAA,WAAA,EAAA;AAHA,WAAA,CAAA,CAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA,EADA,CAEA;AACA;AACA;AACA,WATA;AAUA,SAjBA,MAiBA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AACA,iBAAA,KAAA;AACA;AACA,OAtBA;AAuBA,KAhCA;AAiCA,IAAA,SAjCA,qBAiCA,QAjCA,EAiCA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,WAAA;AACA,KAnCA;AAoCA;AACA,IAAA,KArCA,mBAqCA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,IAAA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,MAAA;AACA,MAAA,QAAA,CAAA,MAAA,GAHA,CAIA;AACA;AA1CA;AA/CA,CAAA","sourcesContent":["<template>\n    <div class=\"info-box\">\n        <div class=\"user-avatar\">\n            <img :src=user.avatar alt=\"avatar\" class=\"ava-img\">\n        </div>\n        <div class=\"user-info\">\n            <div class=\"half-info\">\n                <div class=\"info-item\">姓名：{{user.username}}</div>\n                <div class=\"info-item\">昵称：{{user.nickname}}</div>\n                <div class=\"info-item\">密码：******\n                    <el-button type=\"text\" style=\"margin-left:10px\" @click=\"dialogFormVisible = true\">修改</el-button>\n                </div>\n            </div>\n            <div class=\"half-info\">\n                <div class=\"info-item\">学校：{{user.school}}</div>\n                <div class=\"info-item\">性别：{{sex}}</div>\n            </div>\n        </div>\n\n        <!-- 修改密码弹框 -->\n        <el-dialog title=\"修改密码\" :visible.sync=\"dialogFormVisible\">\n            <el-form :model=\"ruleForm\" status-icon :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\n                <el-form-item label=\"请输入新密码\" prop=\"pass\">\n                    <el-input type=\"password\" v-model=\"ruleForm.pass\" autocomplete=\"off\"></el-input>\n                </el-form-item>\n                <el-form-item label=\"确认新密码\" prop=\"checkPass\">\n                    <el-input type=\"password\" v-model=\"ruleForm.checkPass\" autocomplete=\"off\"></el-input>\n                </el-form-item>\n                <el-form-item>\n                    <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">提交</el-button>\n                    <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\n                </el-form-item>\n            </el-form>\n        </el-dialog>\n    </div>\n</template>\n\n<script>\nimport { put } from '../utils/fetch.js'\nexport default {\n    name: 'info',\n    data() {\n      var validatePass = (rule, value, callback) => {\n        if (value === '') {\n          callback(new Error('请输入密码'));\n        } else {\n          if (this.ruleForm.checkPass !== '') {\n            this.$refs.ruleForm.validateField('checkPass');\n          }\n          callback();\n        }\n      };\n      var validatePass2 = (rule, value, callback) => {\n        if (value === '') {\n          callback(new Error('请再次输入密码'));\n        } else if (value !== this.ruleForm.pass) {\n          callback(new Error('两次输入密码不一致!'));\n        } else {\n          callback();\n        }\n      };\n      return {\n\n        user: {},\n        sex:'',\n        dialogFormVisible: false,\n        formLabelWidth: '120px',\n                \n        ruleForm: {\n        pass: '',\n        checkPass: '',\n        },\n        rules: {\n          pass: [\n            { validator: validatePass, trigger: 'blur' }\n          ],\n          checkPass: [\n            { validator: validatePass2, trigger: 'blur' }\n          ],\n        }\n      };\n    },\n    created(){\n        this.getLocal()\n        this.getSex()\n    },\n    methods: {\n        getLocal(){\n            this.user = JSON.parse(localStorage.getItem('user'))\n        },\n        // 显示性别\n        getSex(){\n            this.sex = this.user.gender=='0' ? '女' : '男'\n        },\n        submitForm(formName) {\n            this.$refs[formName].validate((valid) => {\n                if (valid) {\n                    console.log('old',this.user.password)\n                    console.log('new',this.ruleForm.checkPass)\n                    \n                    let oldp = this.user.password;\n                    let newp = this.ruleForm.checkPass;\n                    \n                    put('/user/changePassword',{\n                        // oldPassword,newPassword\n                        oldPassword: oldp,\n                        newPassword: newp,\n                    }).then(res => {\n                        console.log(res);\n                        // alert('密码已经修改，请重新登录！');\n                        // this.clear();\n                        // this.$router.push('/login2');\n                    });\n                } else {\n                    console.log('error submit!!');\n                    return false;\n                }\n            });\n        },\n        resetForm(formName) {\n            this.$refs[formName].resetFields();\n        },\n        // 清除缓存\n        clear(){\n            localStorage.removeItem('id');\n            localStorage.removeItem('user');\n            location.reload();\n        // this.$router.go(0);\n        },\n    },\n}\n</script>\n\n<style scoped lang=\"scss\">\n.info-box{\n    margin: 10px;\n    \n    position: relative;\n    display: flex;\n    .user-avatar{\n        width: 250px;\n        height: 400px;\n        text-align: center;\n        \n        .ava-img{\n            width: 90px;\n            height: 90px;\n            margin: 20px auto;\n            border-radius: 50%;\n        }\n    };\n    .user-info{\n        width: 400px;\n        height: 400px;\n        position: relative;\n        display: flex;\n        .half-info{\n            width: 200px;\n            height: 400px;\n            .info-item{\n                margin: 20px 40px 10px 15px;\n            }\n        }\n    }\n}\n</style>"],"sourceRoot":"src/views"}]}